{
  "name": "mcp-client-langchain-ts",
  "version": "0.1.20",
  "description": "MCP Client Using LangChain / TypeScript",
  "license": "MIT",
  "keywords": [
    "modelcontextprotocol",
    "mcp",
    "mcp-client",
    "langchain",
    "langchain-tyoescript",
    "tool-calling",
    "typescript",
    "nodejs"
  ],
  "author": "passive-radio",
  "type": "module",
  "scripts": {
    "start": "tsx src/index.ts",
    "start:h": "tsx src/index.ts -h",
    "start:v": "tsx src/index.ts -v",
    "dev": "tsx watch src/index.ts",
    "lint": "eslint src",
    "clean": "git clean -fdxn -e .env && read -p 'OK?' && git clean -fdx -e .env",
    "web:build": "cd src/web-ui && vite build",
    "web:dev": "cd src/web-ui && vite",
    "web:server": "tsx src/web-server/server.ts",
    "web": "npm run web:build && npm run web:server"
  },
